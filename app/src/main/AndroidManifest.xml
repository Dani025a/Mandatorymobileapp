<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mandatorymobileapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MandatoryMobileApp">
        <activity
            android:name="com.example.mandatorymobileapp.UI.fragments.RegisterFragment"
            android:exported="false" />
        <activity
            android:name="com.example.mandatorymobileapp.UI.fragments.LoginFragment"
            android:exported="false" />
        <activity
            android:name="com.example.mandatorymobileapp.UI.fragments.LoginRegisterFragment"
            android:exported="false" />
        <activity
            android:name="com.example.mandatorymobileapp.UI.fragments.List_Fragment"
            android:exported="false" />
        <activity
            android:name="com.example.mandatorymobileapp.UI.fragments.ItemFragment"
            android:exported="false" />
        <activity
            android:name="com.example.mandatorymobileapp.UI.fragments.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>